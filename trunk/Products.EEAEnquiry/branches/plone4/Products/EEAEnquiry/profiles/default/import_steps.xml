<?xml version="1.0"?>
<import-steps>

	<import-step 
		id="eeaenquiry-types" 
		handler="Products.EEAEnquiry.setuphandlers.installTypesCatalog"
		title="EEAEnquiry Types Cataloging"
		version="1">
		Setting custom cataloging for types
		<dependency step="typeinfo"/>
	</import-step>

	<import-step 
		id="eeaenquiry-catalog" 
		handler="Products.EEAEnquiry.setuphandlers.installCatalogIndexes"
		title="EEAEnquiry catalog tool indexes"
		version="2">
		Setting cataloging indexes for types
		<dependency step="toolset"/>
	</import-step>

</import-steps>
